@section pagespecific{
    <script type="text/javascript">
        $("#addDoctor").click(function () {
            var content = '<div class="col-sm-6 eq-box-sm"> <div class="panel" >';
            content = +'<div class="panel-heading"><div class="panel-control">';
            content = +' <button class="btn btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></button>';
            content = +' <button class="btn btn-default" data-click="panel-reload"><i class="fa fa-refresh"></i></button>';
            content = +' <button class="btn btn-default" data-click="panel-collapse"><i class="fa fa-chevron-down"></i></button>';
            content = +' <button class="btn btn-default" data-dismiss="panel"><i class="fa fa-times"></i></button>';
            content = +' </div> ';
            content = +' <h3 class="panel-title" > Block styled form</h3>';
            content = +'</div >';
            $("#modalContent").html(content);
            $("#divModal").modal('show');
        });
    </script>
}