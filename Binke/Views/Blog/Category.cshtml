        @foreach (var subItem in Model.lstCategories)
        {

            if (Model.lstbundles.Where(i => i.SubCategoryId == subItem.CategoryId).ToList().Count > 0)
            {
                <div id="@subItem.CategoryName" class="e13-7 x-section">
                    <div class="row">
                        <div class="e13-10 x-text b-page-title x-text-headline col-md-12">
                            <div class="x-text-content">
                                <div class="x-text-content-text">
                                    <div class="x-text-content-text-primary">
                                        <a href="/Blog/SubCategory/@subItem.CategoryName.Replace(' ', '-').Replace('&', '-')">@subItem.CategoryName</a>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">

                        <div class="carousel-wrap" style="width:90%;">
                            <div class="owl-carousel owl-theme">
                                @foreach (var item in Model.lstbundles.Where(i => i.SubCategoryId == subItem.CategoryId))
                                {
                                    <div class="item sliderBorder">
                                        <div class="esg-media-cover-wrapper">
