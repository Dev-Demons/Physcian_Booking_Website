                                            <div class="esg-content eg-post-9589 eg-petcentral-grid1-element-4-a">
                                                <a class="eg-petcentral-grid1-element-4 eg-post-9589" href="/Blog/Category/@Model.CategoryName.Replace(" ","-").Replace("&","-")" @*href="@Url.Action("Category", "Blog", new { CategoryId = Model.CategoryId })"*@
                                                   title="View all posts in @Model.CategoryName" rel="category tag">@Model.CategoryName</a> /
                                                <a class="eg-petcentral-grid1-element-4 eg-post-9589" href="/Blog/SubCategory/@subItem.CategoryNameReplace" @*href="@Url.Action("SubCategory", "Blog", new { SubCategoryId = subItem.CategoryId })"*@
                                                   title="View all posts in @subItem.CategoryName" rel="category tag">@subItem.CategoryName</a>
                                            </div>
                                            <div class="esg-content eg-post-9589 eg-petcentral-grid1-element-1-a"><a class="eg-petcentral-grid1-element-1 eg-post-9589" href="/Blog/BlogItem/@item.ArticleNameReplace" @*href="@Url.Action("BlogItem", "Blog", new { BlogItem = item.CategoryId })"*@ target="_self">@item.ArticleName</a></div>
                                            <div class="esg-content eg-post-9589 eg-petcentral-grid1-element-5"><span style="display: block;min-height: 75px;overflow: hidden; height: 75px; max-height: 75px">@item.ArticleSummary</span></div>
                                        </div>

                                        @*<img src="~/Images/images1.jpg" />*@
                                        @*<span class="img-text">nightlife</span>*@
                                    </div>
                                }

                            </div>
                        </div>


                    </div>
                </div>
                <div class="clear"></div>
                <div style="display: flex;justify-content: center;align-items: center;">
                    <a class="e13-12 x-anchor x-anchor-button" href="/Blog/SubCategory/@subItem.CategoryName.Replace(' ', '-').Replace('&', '-')">
                        <span class="x-anchor-content">

                            <span class="x-anchor-text"><span class="x-anchor-text-primary">See More</span></span>
                            <span class="x-graphic" aria-hidden="true">
